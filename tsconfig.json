{
  /* Base configuration. Do not edit this field. */
  "extends": "./temp/tsconfig.cocos.json",

  /* Add your custom configuration here. */
  "compilerOptions": {
    "lib": [
      "es2015",
      "es2017",
      "dom",
      "webworker",
      "es2018.promise",
      "ESNext.BigInt"
    ],
    "target": "es5",
    "skipLibCheck": true,
    "strict": false,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true, // needs to be turned on
  },
}
